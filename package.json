{
  "name": "app-boilerplate",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "MapColonies React TypeScript Vite boilerplate with Cesium, i18n, routing, and Helm",
  "repository": {
    "type": "git",
    "url": "https://github.com/MapColonies/app-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run copyassets:all",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "copyassets:all": "npm run copyassets:cesium",
    "copyassets:cesium": "copyfiles -u 3 \"./node_modules/cesium/Build/Cesium/**/*\" \"./dist\"",
    "confd": "node ./confd/generate-config.js",
    "confd:prod": "node ./confd/generate-config.js --environment production",
    "confd:prod:indocker": "node ./confd/generate-config.js --environment production --indocker",
    "prepare": "husky install"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@types/lodash": "^4.17.23",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.13.2",
    "cesium": "^1.137.0",
    "copyfiles": "^2.4.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^5.7.1",
    "react-router-dom": "^5.3.4",
    "vite-plugin-cesium": "^1.2.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
